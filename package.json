{
  "name": "lastfm-similar-graph",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "typecheck": "tsc -p . --noEmit",
    "lint": "eslint --ext .ts,.tsx src/",
    "format": "prettier -w ./src",
    "format:check": "prettier -c \"./src/**/*.(js|jsx|ts|tsx)\""
  },
  "dependencies": {
    "cytoscape": "^3.21.0",
    "fast-xml-parser": "^4.0.2",
    "md5": "^2.3.0",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^4.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@preact/preset-vite": "^2.1.7",
    "@types/cytoscape": "^3.19.4",
    "@types/enzyme": "^3.10.11",
    "@types/jest": "^27.4.0",
    "@types/md5": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^3.3.0",
    "eslint": "^8.8.0",
    "eslint-config-preact": "^1.1.0",
    "jest": "^27.5.1",
    "jest-preset-preact": "^4.0.5",
    "preact-cli": "^3.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
